{
  "name": "purple",
  "private": true,
  "description": "A Remix starter with Prisma",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "ci": "npm run lint && npm run ts-check && npx playwright install && npm run test",
    "clean": "rm -rf .eslintcache playwright-report build public/build .cache node_modules test-results",
    "deploy": "npm run ts-run tasks/validateConfig.ts && prisma generate && prisma db push && npm run build",
    "dev": "remix vite:dev",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache",
    "prepare": "[ \"$NODE_ENV\" != \"production\" ] && husky || :",
    "start": "remix-serve ./build/server/index.js",
    "test": "npm run test:vitest && npm run test:e2e",
    "test:e2e": "dotenv -e .env.test -- bash -c 'prisma db push -- --force && npm run build && playwright test'",
    "test:e2e-quick": "dotenv -e .env.test -- bash -c 'playwright test'",
    "test:vitest": "vitest --run",
    "test:vitest:watch": "vitest",
    "ts-check": "tsc --project tsconfig.json --noEmit --skipLibCheck",
    "ts-run": "dotenv tsx",
    "studio": "prisma studio",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "npm run ts-run prisma/seed.ts",
    "db:migrate:force": "prisma migrate reset --force",
    "generate": "prisma generate",
    "docker:db": "docker compose -f docker-compose.yml up -d",
    "validate-env": "npm run ts-run tasks/validateConfig.ts"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --cache --fix"
  },
  "dependencies": {
    "@blocknote/core": "^0.14.5",
    "@blocknote/mantine": "^0.14.6",
    "@blocknote/react": "^0.14.6",
    "@mantine/carousel": "^7.11.2",
    "@mantine/core": "^7.11.2",
    "@mantine/hooks": "^7.11.2",
    "@mantine/notifications": "^7.11.2",
    "@prisma/client": "5.16.1",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@remix-run/node": "^2.10.2",
    "@remix-run/react": "2.10.2",
    "@remix-run/serve": "^2.10.2",
    "@tabler/icons-react": "^3.10.0",
    "@uidotdev/usehooks": "^2.4.1",
    "accept-language-parser": "^1.5.0",
    "argon2": "^0.40.3",
    "class-variance-authority": "^0.7.0",
    "date-fns": "^3.6.0",
    "isbot": "^5.1.11",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.400.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remix": "2.10.2",
    "zod": "^3.23.8",
    "zod-error": "^1.5.0",
    "zod-form-data": "^2.0.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright-testing-library/test": "^4.5.0",
    "@playwright/test": "^1.45.1",
    "@remix-run/dev": "^2.10.2",
    "@remix-run/eslint-config": "^2.10.2",
    "@remix-run/testing": "^2.10.2",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/accept-language-parser": "^1.5.6",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-testing-library": "^6.2.2",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.3.2",
    "prisma": "5.16.1",
    "remix-flat-routes": "^0.6.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=20.15.0"
  }
}
